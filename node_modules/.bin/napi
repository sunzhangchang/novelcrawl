#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/.pnpm/@napi-rs+cli@2.4.2/node_modules/@napi-rs/cli/scripts/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/.pnpm/@napi-rs+cli@2.4.2/node_modules/@napi-rs/cli/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/.pnpm/@napi-rs+cli@2.4.2/node_modules/@napi-rs/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/.pnpm/@napi-rs+cli@2.4.2/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/.pnpm/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/node_modules:/mnt/e/szc/Workspace/Web/node_modules:/mnt/e/szc/Workspace/node_modules:/mnt/e/szc/node_modules:/mnt/e/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/@napi-rs/cli/scripts/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/@napi-rs/cli/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/@napi-rs/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/.pnpm/@napi-rs+cli@2.4.2/node_modules/@napi-rs/cli/scripts/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/.pnpm/@napi-rs+cli@2.4.2/node_modules/@napi-rs/cli/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/.pnpm/@napi-rs+cli@2.4.2/node_modules/@napi-rs/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/.pnpm/@napi-rs+cli@2.4.2/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/.pnpm/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/node_modules:/mnt/e/szc/Workspace/Web/node_modules:/mnt/e/szc/Workspace/node_modules:/mnt/e/szc/node_modules:/mnt/e/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/@napi-rs/cli/scripts/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/@napi-rs/cli/node_modules:/mnt/e/szc/Workspace/Web/vscreboook/vscrebook/node_modules/@napi-rs/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@napi-rs/cli/scripts/index.js" "$@"
else
  exec node  "$basedir/../@napi-rs/cli/scripts/index.js" "$@"
fi
